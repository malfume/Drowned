print("Starting script 1")
local keyString = _G.Key -- Use the global key

-- Load the KeyLibrary
local KeyLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/MaGiXxScripter0/keysystemv2api/master/version2_2.lua"))()
print("Loaded KeyLibrary")

-- Set KeyLibrary parameters
KeyLibrary.Set({
    ApplicationName = "UruyuHub",
    AuthType = "clientid",
    TrueData = "true2",
    FalseData = "false1",
    Debug = true
})
print("Set KeyLibrary parameters")

-- Verify the key
local o_data = KeyLibrary.VerifyPremiumKey(keyString)
print("Verified key, result: " .. tostring(o_data))

-- Handle the verification result
if o_data == "true2" then
    print("Key is valid!")
    local scriptUrls = {
        [14070029709] = "https://raw.githubusercontent.com/malfume/Drowned/main/DrownedHubTypesoul",
        [14071822972] = "https://raw.githubusercontent.com/malfume/Drowned/main/DrownedHubTypesoul",
        [14069122388] = "https://raw.githubusercontent.com/malfume/Drowned/main/DrownedHubTypesoul",
        [14069678431] = "https://raw.githubusercontent.com/malfume/Drowned/main/DrownedHubTypesoul",
        [14069866342] = "https://raw.githubusercontent.com/malfume/Drowned/main/DrownedHubTypesoul",
        [14069956183] = "https://raw.githubusercontent.com/malfume/Drowned/main/DrownedHubTypesoul",
        [8501406566] = "https://raw.githubusercontent.com/malfume/Drowned/main/LegacyPiece",
        [14070000001] = "https://raw.githubusercontent.com/malfume/Drowned/main/DrownedHubTypesoul"
    }

    -- Fetch and execute the script
    local scriptUrl = scriptUrls[game.GameId]
    if scriptUrl then
        print("Script URL found for current game ID")
        local scriptContent = game:HttpGet(scriptUrl)
        if scriptContent then
            print("Script content retrieved")
            local scriptFunction, errorMsg = loadstring(scriptContent)
            if scriptFunction then
                print("Script loaded successfully")
                local success, error_msg = pcall(scriptFunction) -- Execute the loaded script
                if not success then
                    print("Failed to execute script:", error_msg)
                else
                    print("Executed script")
                end
            else
                print("Failed to load script:", errorMsg)
            end
        else
            print("Failed to load script content")
        end
    else
        print("No script URL found for current game ID:", game.GameId)
    end
elseif o_data == "false1" then
    print("Key is not valid!")
else
    print("Unknown error occurred during key verification")
end
